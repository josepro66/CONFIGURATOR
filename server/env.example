# Configuración del servidor
NODE_ENV=development
PORT=3001

# Configuración de CORS
CORS_ORIGIN=http://localhost:3000

# Credenciales PayU
PAYU_API_KEY=your_payu_api_key_here
PAYU_MERCHANT_ID=your_payu_merchant_id_here
PAYU_ACCOUNT_ID=your_payu_account_id_here
PAYU_SIGNATURE_KEY=your_payu_signature_key_here
PAYU_BASE_URL=https://sandbox.api.payulatam.com

# Credenciales PayPal
PAYPAL_CLIENT_ID=your_paypal_client_id_here
PAYPAL_CLIENT_SECRET=your_paypal_client_secret_here
PAYPAL_MODE=sandbox

# Configuración de productos y precios
BEATO_PRICE=200.00
BEATO16_PRICE=250.00
KNOBO_PRICE=150.00
MIXO_PRICE=200.00
LOOPO_PRICE=110.00
FADO_PRICE=150.00

# Configuración de seguridad
JWT_SECRET=your_jwt_secret_here
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Configuración de webhooks
WEBHOOK_SECRET=your_webhook_secret_here

